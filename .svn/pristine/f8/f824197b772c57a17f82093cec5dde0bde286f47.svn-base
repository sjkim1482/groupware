package kr.or.ddit.groupware.model;

import java.text.SimpleDateFormat;
import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

public class VacationVo {
	
	private int vac_no;
	@DateTimeFormat(pattern = "yyyy-MM-dd")
	private Date s_dt;
	@DateTimeFormat(pattern = "yyyy-MM-dd")
	private Date e_dt;
	private int emp_no;
	private int vac_cd;
	private int vacationday;
	private String ko_nm;
	private String dept_nm;
	private String cd_ko_nm;
	private String hire_dt;
	private String retire_dt;
	private int work_year;
	private double used_vac;
	private int mon_vac;
	private int total_mon_vac;
	private int total_vac;
	private int remain_vac;
	private int emp_no_b;
	private String emp_id;
	
	public String getStringS_dt() {
		SimpleDateFormat sdf =  new SimpleDateFormat("yyyy-MM-dd");
		
		return sdf.format(this.s_dt);
	}
	
	public String getStringE_dt() {
		SimpleDateFormat sdf =  new SimpleDateFormat("yyyy-MM-dd");
		
		return sdf.format(this.e_dt);
	}
	
	public String getEmp_id() {
		return emp_id;
	}
	public void setEmp_id(String emp_id) {
		this.emp_id = emp_id;
	}
	public int getEmp_no_b() {
		return emp_no_b;
	}
	public void setEmp_no_b(int emp_no_b) {
		this.emp_no_b = emp_no_b;
	}
	public String getKo_nm() {
		return ko_nm;
	}
	public void setKo_nm(String ko_nm) {
		this.ko_nm = ko_nm;
	}
	public String getDept_nm() {
		return dept_nm;
	}
	public void setDept_nm(String dept_nm) {
		this.dept_nm = dept_nm;
	}
	public String getCd_ko_nm() {
		return cd_ko_nm;
	}
	public void setCd_ko_nm(String cd_ko_nm) {
		this.cd_ko_nm = cd_ko_nm;
	}

	
	public String getHire_dt() {
		return hire_dt;
	}
	public void setHire_dt(String hire_dt) {
		this.hire_dt = hire_dt;
	}
	public String getRetire_dt() {
		return retire_dt;
	}
	public void setRetire_dt(String retire_dt) {
		this.retire_dt = retire_dt;
	}
	public int getWork_year() {
		return work_year;
	}
	public void setWork_year(int work_year) {
		this.work_year = work_year;
	}
	public double getUsed_vac() {
		return used_vac;
	}
	public void setUsed_vac(double used_vac) {
		this.used_vac = used_vac;
	}
	public int getMon_vac() {
		return mon_vac;
	}
	public void setMon_vac(int mon_vac) {
		this.mon_vac = mon_vac;
	}
	public int getTotal_mon_vac() {
		return total_mon_vac;
	}
	public void setTotal_mon_vac(int total_mon_vac) {
		this.total_mon_vac = total_mon_vac;
	}
	public int getTotal_vac() {
		return total_vac;
	}
	public void setTotal_vac(int total_vac) {
		this.total_vac = total_vac;
	}
	public int getRemain_vac() {
		return remain_vac;
	}
	public void setRemain_vac(int remain_vac) {
		this.remain_vac = remain_vac;
	}
	public int getVacationday() {
		return vacationday;
	}
	public void setVacationday(int vacationday) {
		this.vacationday = vacationday;
	}
	public int getVac_no() {
		return vac_no;
	}
	public void setVac_no(int vac_no) {
		this.vac_no = vac_no;
	}
	public Date getS_dt() {
		return s_dt;
	}
	public void setS_dt(Date s_dt) {
		this.s_dt = s_dt;
	}
	public Date getE_dt() {
		return e_dt;
	}
	public void setE_dt(Date e_dt) {
		this.e_dt = e_dt;
	}
	public int getEmp_no() {
		return emp_no;
	}
	public void setEmp_no(int emp_no) {
		this.emp_no = emp_no;
	}
	public int getVac_cd() {
		return vac_cd;
	}
	public void setVac_cd(int vac_cd) {
		this.vac_cd = vac_cd;
	}
	@Override
	public String toString() {
		return "VacationVo [vac_no=" + vac_no + ", s_dt=" + s_dt + ", e_dt=" + e_dt + ", emp_no=" + emp_no + ", vac_cd="
				+ vac_cd + ", vacationday=" + vacationday + ", ko_nm=" + ko_nm + ", dept_nm=" + dept_nm + ", cd_ko_nm="
				+ cd_ko_nm + ", hire_dt=" + hire_dt + ", retire_dt=" + retire_dt + ", work_year=" + work_year
				+ ", used_vac=" + used_vac + ", mon_vac=" + mon_vac + ", total_mon_vac=" + total_mon_vac
				+ ", total_vac=" + total_vac + ", remain_vac=" + remain_vac + ", emp_no_b=" + emp_no_b + ", emp_id="
				+ emp_id + "]";
	}

	
	
	
	

}
