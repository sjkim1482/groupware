package kr.or.ddit.groupware.model;

import java.util.Date;
import java.util.List;

import org.springframework.format.annotation.DateTimeFormat;

public class MsgVo {

	private int msg_no;
	private String title;
	private String cont;
	@DateTimeFormat(pattern = "yyyy-MM-dd")
	private Date reg_dt;
	private int higher_msg_no;
	private int sd_emp_no;
	private int rc_emp_no;
	private int msg_sta_cd;
	private String rc_sta_cd;
	private int emp_no;
	private int msg_box_no;
	private List<Integer> msgEmpList;
	private String emp_id;
	private String ko_nm; 
	private String dept_nm;
	private String cd_ko_nm;
	
	
	
	public String getCd_ko_nm() {
		return cd_ko_nm;
	}
	public void setCd_ko_nm(String cd_ko_nm) {
		this.cd_ko_nm = cd_ko_nm;
	}
	public String getKo_nm() {
		return ko_nm;
	}
	public void setKo_nm(String ko_nm) {
		this.ko_nm = ko_nm;
	}
	public String getDept_nm() {
		return dept_nm;
	}
	public void setDept_nm(String dept_nm) {
		this.dept_nm = dept_nm;
	}
	public String getEmp_id() {
		return emp_id;
	}
	public void setEmp_id(String emp_id) {
		this.emp_id = emp_id;
	}
	public List<Integer> getMsgEmpList() {
		return msgEmpList;
	}
	public void setMsgEmpList(List<Integer> msgEmpList) {
		this.msgEmpList = msgEmpList;
	}
	public int getMsg_no() {
		return msg_no;
	}
	public void setMsg_no(int msg_no) {
		this.msg_no = msg_no;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getCont() {
		return cont;
	}
	public void setCont(String cont) {
		this.cont = cont;
	}
	public Date getReg_dt() {
		return reg_dt;
	}
	public void setReg_dt(Date reg_dt) {
		this.reg_dt = reg_dt;
	}
	public int getHigher_msg_no() {
		return higher_msg_no;
	}
	public void setHigher_msg_no(int higher_msg_no) {
		this.higher_msg_no = higher_msg_no;
	}
	public int getSd_emp_no() {
		return sd_emp_no;
	}
	public void setSd_emp_no(int sd_emp_no) {
		this.sd_emp_no = sd_emp_no;
	}
	public int getRc_emp_no() {
		return rc_emp_no;
	}
	public void setRc_emp_no(int rc_emp_no) {
		this.rc_emp_no = rc_emp_no;
	}
	public int getMsg_sta_cd() {
		return msg_sta_cd;
	}
	public void setMsg_sta_cd(int msg_sta_cd) {
		this.msg_sta_cd = msg_sta_cd;
	}
	public String getRc_sta_cd() {
		return rc_sta_cd;
	}
	public void setRc_sta_cd(String rc_sta_cd) {
		this.rc_sta_cd = rc_sta_cd;
	}
	public int getEmp_no() {
		return emp_no;
	}
	public void setEmp_no(int emp_no) {
		this.emp_no = emp_no;
	}
	public int getMsg_box_no() {
		return msg_box_no;
	}
	public void setMsg_box_no(int msg_box_no) {
		this.msg_box_no = msg_box_no;
	}
	@Override
	public String toString() {
		return "MsgVo [msg_no=" + msg_no + ", title=" + title + ", cont=" + cont + ", reg_dt=" + reg_dt
				+ ", higher_msg_no=" + higher_msg_no + ", sd_emp_no=" + sd_emp_no + ", rc_emp_no=" + rc_emp_no
				+ ", msg_sta_cd=" + msg_sta_cd + ", rc_sta_cd=" + rc_sta_cd + ", emp_no=" + emp_no + ", msg_box_no="
				+ msg_box_no + ", msgEmpList=" + msgEmpList + ", emp_id=" + emp_id + ", ko_nm=" + ko_nm + ", dept_nm="
				+ dept_nm + ", cd_ko_nm=" + cd_ko_nm + "]";
	}
	
	
	
	
	
	
	

}
