		            WHEN c.comm_no = 201 THEN (v.e_dt - v.s_dt) + 0.5
		            WHEN c.comm_no = 202 THEN (v.e_dt - v.s_dt) + 0.5 ELSE (e_dt - s_dt) + 1 
		            END used_vac
